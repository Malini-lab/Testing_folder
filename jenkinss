pipeline {
    agent any   // Or use agent { label 'selenium-node' } if you have a specific node with Chrome installed

    stages {
        stage('pulling the selenium code from the git hub') {
            steps {
                // Pull your Selenium test code from Git
                git branch: 'main', url: 'https://github.com/Malini-lab/Testing_folder.git/lunchingchrome.py'
            }
        }

        stage('Lunching the chrome') {
            steps {

                    // Example: run a Python script that launches Chrome
                    python lunchingchrome.py
            }
        }
    }
}
